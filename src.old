    <script>
          // Store the current UTC datetime in ISO string format
          function updateDateTime() {
              const currentUTCDateTime = new Date().toISOString(); // UTC time
              localStorage.setItem('localizationLastUpdated', currentUTCDateTime);
              console.log('Date and Time Updated:', currentUTCDateTime);
          }

          // Check if more than 24 hours have passed since the last update
          function hasOneDayPassed() {
              const lastUpdate = localStorage.getItem('localizationLastUpdated');
              if (!lastUpdate) {
                  console.log('No previous update found.');
                  return true; // Treat as expired if no update found
              }

              const lastDate = new Date(lastUpdate); // Stored UTC time
              const now = new Date(); // Current time in UTC

              // Calculate the difference in milliseconds
              const differenceInMillis = now.getTime() - lastDate.getTime();

              // Convert milliseconds to hours
              const hoursPassed = differenceInMillis / (1000 * 60 * 60);
              console.log(hoursPassed)
              console.log(`Hours since last update: ${hoursPassed}`);

