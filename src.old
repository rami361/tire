    <script>
      let originalFaviconHref = 'data:image/png;base64,'
      let dotFaviconHref = 'data:image/png;base64,'

      changeFavicon(originalFaviconHref);
      document.addEventListener("visibilitychange", function () {
        if (document.visibilityState === "hidden") {
          // Change favicon when user leaves the page
          changeFavicon(dotFaviconHref);
        } else {
          // Revert to original favicon when user returns
          changeFavicon(originalFaviconHref);
        }
      });

      function changeFavicon(faviconURL) {
        // Remove existing favicon links (both rel='icon' and rel='shortcut icon')
        const existingLinks = document.querySelectorAll("link[rel='icon'], link[rel='shortcut icon']");
        existingLinks.forEach(link => link.parentNode.removeChild(link));

        // Create new favicon links
        const iconLink = document.createElement("link");
        iconLink.rel = "icon";
        iconLink.href = faviconURL;
        document.head.appendChild(iconLink);

        const shortcutIconLink = document.createElement("link");
        shortcutIconLink.rel = "shortcut icon";
        shortcutIconLink.href = faviconURL;
        shortcutIconLink.type = "image/png";
        document.head.appendChild(shortcutIconLink);
      }
    </script>
